<?xml version="1.0"?>
<!DOCTYPE module PUBLIC
          "-//Checkstyle//DTD Checkstyle Configuration 1.3//EN"
          "https://checkstyle.org/dtds/configuration_1_3.dtd">

<module name="Checker">
  <property name="severity" value="ignore" />

  <module name="PropertyWalker">
    <module
      name="com.rnveach.tools.checkstyle.extras.checks.property.IllegalTokenPropertyCheck" />
  </module>

  <module name="XmlWalker">
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.AttributeNameXmlCheck" />
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.AttributeOrderXmlCheck" />
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.ElementNameXmlCheck" />
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.EmptyContentXmlCheck" />
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.IllegalTokenTextXmlCheck" />
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.IllegalTokenXmlCheck" />
    <module name="com.rnveach.tools.checkstyle.extras.checks.xml.IndentationXmlCheck" />
  </module>
</module>
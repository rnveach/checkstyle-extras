<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
  <suppress files="[\\/]parsers[\\/]\w+Parser\.java" checks="ClassDataAbstractionCoupling" />

  <suppress files="[\\/]visitors[\\/]\w+AstVisitor\.java" checks="LambdaBodyLength" />

  <suppress files="[\\/]walkers[\\/]\w+Walker\.java" checks="GenericWhitespace"
    message="'>'" />

  <!-- Tests testing specific items -->
  <suppress files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]Input.properties"
    checks="(Ordered|Unique)Properties" />
  <suppress files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputEmpty.properties"
    checks="NewlineAtEndOfFile" />
  <suppress files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputEmpty.properties"
    id="noEmptyFile" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputIndentation.properties"
    id="noTrailingWhitespace" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputKeysAlone.properties"
    checks="OrderedProperties" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputValueContinuationEnd.properties"
    checks="NewlineAtEndOfFile" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputWhitespace.properties"
    id="noConsecutiveLines" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputWhitespace.properties"
    id="noEmptyFile" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputWhitespace.properties"
    id="noTrailingWhitespace" />
  <suppress
    files="[\\/]test[\\/]resources[\\/].*[\\/]printers[\\/]InputWhitespace.properties"
    checks="FileTabCharacter" />

  <!-- Indentation controlled by Eclipse. -->
  <suppress checks="IndentationCheck" />
  <suppress checks="TrailingComment" />
  <!-- Not implementing languages yet. -->
  <suppress checks="Translation" />
</suppressions>
